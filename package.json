    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.16",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.0.0",
    "jose": "^5.1.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.295.0",
    "mysql2": "^3.6.5",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "nodemailer": "^7.0.10",
    "postcss": "^8.4.32",
    "prisma": "^6.18.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
