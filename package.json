{
  "name": "laboratorio-3d-migracion",
  "version": "1.0.0",
  "description": "Sistema serverless para Laboratorio 3D con Next.js y MySQL",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
